#include <iostream>
using namespace std;

int main() {
    int n;
    cout << "Enter the size of the array: ";
    cin >> n;

    int arr[n];
    int variable;
    bool found = false;

    cout << "Enter " << n << " elements of the array:" << endl;
    for (int i = 0; i < n; i++) {
        cin >> arr[i];
    }

    cout << "Enter a number to search: ";
    cin >> variable;

    for (int i = 0; i < n; i++) {
        if (arr[i] == variable) {
            found = true;
            break;
        }
    }

    if (found) {
        cout << "Found!" << endl;
    } else {
        cout << "Not found." << endl;
    }

    return 0;
}
